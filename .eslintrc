{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "eol-last": "off",
    "no-console": "off",
    "no-return-await": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "no-confusing-arrow": "off"
  },
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ]
}